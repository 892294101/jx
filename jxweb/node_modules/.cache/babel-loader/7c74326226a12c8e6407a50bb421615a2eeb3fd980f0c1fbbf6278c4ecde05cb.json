{"ast":null,"code":"import storage from '@/utils/storage';\nimport HeadImage from '@/components/HeadImage';\nimport Tags from '@/components/Tags';\nexport default {\n  name: \"Home\",\n  components: {\n    HeadImage,\n    Tags\n  },\n  data() {\n    return {\n      leftMenuList: storage.getItem(\"leftMenuList\"),\n      activePath: '',\n      collapseBtnClass: \"el-icon-s-fold\",\n      isCollapse: false\n    };\n  },\n  computed: {\n    // 无子集\n    noChildren() {\n      return this.leftMenuList.filter(item => !item.menuSvoList);\n    },\n    // 有子集\n    hasChildren() {\n      return this.leftMenuList.filter(item => item.menuSvoList);\n    }\n  },\n  methods: {\n    // 点击实现跳转\n    saveNavState(activePath) {\n      storage.setItem('activePath', activePath);\n      this.activePath = activePath;\n    },\n    // 张开和折叠\n    toggleCollapse() {\n      this.isCollapse = !this.isCollapse;\n      if (this.isCollapse) {\n        this.collapseBtnClass = 'el-icon-s-unfold';\n      } else {\n        this.collapseBtnClass = 'el-icon-s-fold';\n      }\n    }\n  }\n};","map":{"version":3,"names":["storage","HeadImage","Tags","name","components","data","leftMenuList","getItem","activePath","collapseBtnClass","isCollapse","computed","noChildren","filter","item","menuSvoList","hasChildren","methods","saveNavState","setItem","toggleCollapse"],"sources":["src/views/Home.vue"],"sourcesContent":["<template>\r\n    <el-container class=\"home-container\">\r\n        <el-aside :width=\"isCollapse ? '64px' : '200px'\">\r\n            <div class=\"logo\">\r\n                <img src=\"../assets/image/logo.png\" class=\"siderbar-logo\">\r\n                <h3 v-show=\"!isCollapse\">松鼠后台管理系统</h3>\r\n            </div>\r\n            <el-menu background-color=\"#304156\" text-color=\"#fff\" unique-opened router :default-active=\"$route.path\"\r\n                :collapse=\"isCollapse\" :collapse-transition=\"false\">\r\n                <!--无子集菜单-->\r\n                <el-menu-item :index=\"'/' + item.url\" v-for=\"item in noChildren\" :key=\"item.menuName\"\r\n                    @click=\"saveNavState('/' + item.url)\">\r\n                    <i :class=\"item.icon\"></i>\r\n                    <template slot=\"title\">\r\n                        <span>{{ item.menuName }}</span>\r\n                    </template>\r\n                </el-menu-item>\r\n                <!--有子集菜单-->\r\n                <el-submenu :index=\"item.id + ''\" v-for=\"item in hasChildren\" :key=\"item.id\">\r\n                    <template slot=\"title\">\r\n                        <i :class=\"item.icon\"></i>\r\n                        <span>{{ item.menuName }}</span>\r\n                    </template>\r\n                    <el-menu-item :index=\"'/' + subItem.url\" v-for=\"subItem in item.menuSvoList\" :key=\"subItem.id\"\r\n                        @click=\"saveNavState('/' + subItem.url)\">\r\n                        <template slot=\"title\">\r\n                            <i :class=\"subItem.icon\"></i>\r\n                            <span>{{ subItem.menuName }}</span>\r\n                        </template>\r\n                    </el-menu-item>\r\n                </el-submenu>\r\n            </el-menu>\r\n        </el-aside>\r\n        <el-container>\r\n            <el-header>\r\n                <div class=\"fold-btn\">\r\n                    <i :class=\"collapseBtnClass\" @click=\"toggleCollapse\"></i>\r\n                </div>\r\n                <div class=\"bread-btn\">\r\n                    <el-breadcrumb separator=\"/\" v-if=\"$router.currentRoute.path != '/welcome'\">\r\n                        <el-breadcrumb-item :to=\"{ path: '/welcome' }\">首页</el-breadcrumb-item>\r\n                        <el-breadcrumb-item>{{ $route.meta.sTitle }}</el-breadcrumb-item>\r\n                        <el-breadcrumb-item>{{ $route.meta.tTitle }}</el-breadcrumb-item>\r\n                    </el-breadcrumb>\r\n                    <el-breadcrumb separator=\"/\" v-else>\r\n                        <el-breadcrumb-item :to=\"{ path: '/welcome' }\">首页</el-breadcrumb-item>\r\n                    </el-breadcrumb>\r\n                </div>\r\n                <HeadImage />\r\n            </el-header>\r\n            <Tags />\r\n            <el-main><router-view /></el-main>\r\n        </el-container>\r\n    </el-container>\r\n</template>\r\n\r\n<script>\r\nimport storage from '@/utils/storage'\r\nimport HeadImage from '@/components/HeadImage'\r\nimport Tags from '@/components/Tags'\r\n\r\nexport default {\r\n    name: \"Home\",\r\n    components: { HeadImage, Tags },\r\n    data() {\r\n        return {\r\n            leftMenuList: storage.getItem(\"leftMenuList\"),\r\n            activePath: '',\r\n            collapseBtnClass: \"el-icon-s-fold\",\r\n            isCollapse: false,\r\n\r\n        }\r\n    },\r\n    computed: {\r\n        // 无子集\r\n        noChildren() {\r\n            return this.leftMenuList.filter(item => !item.menuSvoList)\r\n        },\r\n        // 有子集\r\n        hasChildren() {\r\n            return this.leftMenuList.filter(item => item.menuSvoList)\r\n        }\r\n    },\r\n    methods: {\r\n        // 点击实现跳转\r\n        saveNavState(activePath) {\r\n            storage.setItem('activePath', activePath)\r\n            this.activePath = activePath\r\n        },\r\n        // 张开和折叠\r\n        toggleCollapse() {\r\n            this.isCollapse = !this.isCollapse\r\n            if (this.isCollapse) {\r\n                this.collapseBtnClass = 'el-icon-s-unfold'\r\n            } else {\r\n                this.collapseBtnClass = 'el-icon-s-fold'\r\n            }\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.home-container {\r\n    height: 100%;\r\n\r\n    .el-aside {\r\n        background-color: #304156;\r\n\r\n        .logo {\r\n            margin-top: 5px;\r\n            display: flex;\r\n            align-items: center;\r\n            font-size: 13px;\r\n            height: 50px;\r\n            color: #fff;\r\n            font-style: italic;\r\n\r\n            .siderbar-logo {\r\n                width: 32px;\r\n                height: 32px;\r\n                margin: 0 16px;\r\n            }\r\n        }\r\n\r\n        .el-menu {\r\n            border-right: none;\r\n        }\r\n    }\r\n\r\n    .el-header {\r\n        background-color: #f9fafc;\r\n        align-items: center;\r\n        justify-content: space-between;\r\n        display: flex;\r\n\r\n        .fold-btn {\r\n            padding-top: 2px;\r\n            font-size: 23px;\r\n            cursor: pointer;\r\n        }\r\n\r\n        .bread-btn {\r\n            padding-top: 2px;\r\n            position: fixed;\r\n            margin-left: 40px;\r\n        }\r\n    }\r\n\r\n    .el-main {\r\n        background-color: #eaedf1;\r\n    }\r\n}\r\n</style>"],"mappings":"AAyDA,OAAAA,OAAA;AACA,OAAAC,SAAA;AACA,OAAAC,IAAA;AAEA;EACAC,IAAA;EACAC,UAAA;IAAAH,SAAA;IAAAC;EAAA;EACAG,KAAA;IACA;MACAC,YAAA,EAAAN,OAAA,CAAAO,OAAA;MACAC,UAAA;MACAC,gBAAA;MACAC,UAAA;IAEA;EACA;EACAC,QAAA;IACA;IACAC,WAAA;MACA,YAAAN,YAAA,CAAAO,MAAA,CAAAC,IAAA,KAAAA,IAAA,CAAAC,WAAA;IACA;IACA;IACAC,YAAA;MACA,YAAAV,YAAA,CAAAO,MAAA,CAAAC,IAAA,IAAAA,IAAA,CAAAC,WAAA;IACA;EACA;EACAE,OAAA;IACA;IACAC,aAAAV,UAAA;MACAR,OAAA,CAAAmB,OAAA,eAAAX,UAAA;MACA,KAAAA,UAAA,GAAAA,UAAA;IACA;IACA;IACAY,eAAA;MACA,KAAAV,UAAA,SAAAA,UAAA;MACA,SAAAA,UAAA;QACA,KAAAD,gBAAA;MACA;QACA,KAAAA,gBAAA;MACA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}